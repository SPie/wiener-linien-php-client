{
    "name": "spie/wiener-linien-php-client",
    "description": "A PHP Client for the Wiener Linien API",
    "keywords": ["PHP","Client","Wiener Linien","API"],
    "authors" : [
        {
            "name": "Daniel Schobert",
            "email": "d00pl3x@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.3",
        "php-http/message": "^1.6"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "^7.3",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "SPie\\WienerLinien\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
